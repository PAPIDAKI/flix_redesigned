<header id="content-header">
  <h1>Sign In</h1>
</header>
<%=form_tag(session_path) do%>
<fieldset>
  <ol>
    <li>
    <%=label_tag :email%>
    <%= email_field_tag :email ,nil, autofocus: true,required:true%>
    </li>
    <li>
    <%=label_tag :password%>
    <%=password_field_tag :password,nil,required:true%>
    </li>
    <p>
    <%=submit_tag 'Sign In'%>
  </p>
      <p>
        Don't have an acount ? <%=link_to "Sign up!",signup_path%>
      </p>
</fieldset>

<%end%>